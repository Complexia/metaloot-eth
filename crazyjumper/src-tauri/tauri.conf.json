{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Crazy Jumper",
  "version": "0.1.0",
  "identifier": "com.crazyjumper.dev",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Crazy Jumper",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "label": "main"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "deep-link": {
      "mobile": [
        {
          "host": "metaloot",
          "pathPrefix": [
            "/open"
          ]
        }
      ],
      "desktop": {
        "schemes": [
          "crazyjumper"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}